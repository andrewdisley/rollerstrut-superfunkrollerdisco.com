{
  "bracketSpacing": true,
  "printWidth": 100,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "overrides": [
    {
      "files": ".prettierrc",
      "options": {
        "parser": "json"
      }
    },
    {
      "files": "*.md",
      "options": {
        "arrowParens": "avoid",
        "printWidth": 70,
        "proseWrap": "preserve",
        "trailingComma": "none"
      }
    }
  ]
}